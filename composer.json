{
  "name": "ehz/wc-multistep-order",
  "description": "Modern WooCommerce multistep order checkout plugin boilerplate.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "brain/monkey": "^4.0",
    "mockery/mockery": "^1.6",
    "pestphp/pest": "^2.34",
    "pestphp/pest-plugin-mock": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Ehz\\WcMultistepOrder\\": "src/"
    },
    "files": [
      "src/plugin-functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Ehz\\WcMultistepOrder\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
